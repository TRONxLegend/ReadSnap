{
    "name": "ReadSnap",
    "version": "1.0",
    "manifest_version": 3,
    "background": {
        "scripts": ["background.js"]
    },
    "permissions": ["activeTab", "scripting","storage"],
    "action": {
        "default_popup" :"popup.html",
        "default_icon": "icon.jpg",
        "default_title": "ReadSnap"
    }
}